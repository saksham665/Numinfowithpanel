{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/admin.py", 
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/fetch",
      "dest": "/api/index.py"
    },
    {
      "src": "/admin(.*)",
      "dest": "/api/admin.py"
    },
    {
      "src": "/debug-db",
      "dest": "/api/admin.py"
    },
    {
      "src": "/debug-keys",
      "dest": "/api/index.py"
    },
    {
      "src": "/",
      "dest": "/api/admin.py"
    }
  ]
}
